#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n,i;
	    cin>>n;
	    int a[n];
	    for(int i=0;i<n;i++)
	    {
	        cin>>a[i];
	    }
	    int c=0;
	    for (int i=0;i<n;i++)
	    {
	    c=c+a[i];}
	    if(c%2==0)
	    cout<<"yes\n";
	    else
	    cout<<"no\n";
	}
	return 0;
}
