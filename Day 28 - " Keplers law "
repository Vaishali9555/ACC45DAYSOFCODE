#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    float a,b,c,d;
        cin>>a>>b>>c>>d;
	    a=a*a;
	    b=b*b;
	    c=c*c*c;
	    d=d*d*d;
	    if((a/c)==b/d)
	    cout<<"Yes"<<endl;
	    else
	    cout<<"No"<<endl;
	    
	}
	return 0;
	
}
