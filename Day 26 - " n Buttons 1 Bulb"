#include <iostream>
using namespace std;
int main() {
	// your code goes here
	int t;
	cin>>t;
	while (t--)
	{
	    int n,i;
	    int c=0;
	    string s,r;
	    cin>>n,
	    cin>>s>>r;
	    for (int i=0;i<n;i++)
	    {
	        if(s[i]!=r[i])
	        c++;
	    }
	    if(c%2==0)
	    cout<<1<<endl;
	    else 
	    cout<<0<<endl;
	}
	return 0;
}
