#include <iostream>
using namespace std;
int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	  int n;
	  string s;
	  int c=0;
	  cin>>n;
	  cin>>s;
	  bool flag = true;
	  for(int i=0;i<n;i++)
	  {
      if (s[i]=='a'|| s[i]=='e' ||s[i]=='i'||s[i]=='o'||s[i]=='u')
           {c=0;}
      else
           { c++;}
            if (c==4)
	          { flag=false;
	            break;}
	    
	  }
	  if(flag==false)
	     cout<<"no\n";
	  else 
	     cout<<"yes\n";
	    
	}
	return 0;
}
