#include <iostream>
using namespace std;
int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--)
	{
	    int n,k;
	    cin>>n>>k;
	    if(k==1 && n%4==0)
	    cout<<"on\n";
	    else if (k==1 && n%4!=0)
	    cout<<"ambiguous\n";
	    else if (k==0 && n%4==0)
	    cout<<"off\n";
	    else 
	    cout<<"on\n";
	}
	return 0;
}
